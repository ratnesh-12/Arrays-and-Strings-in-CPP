#include <iostream>
using namespace std;

int main() {
    int n, i;
    cout << "Enter number of elements: ";
    cin >> n;
    int arr[n];
    cout << "Enter the elements:";
    for (i=0; i<n; i++) {
        cin>>arr[i];
    }
    int sum=0;
    for (i=0; i<n; i++) {
        sum = sum+arr[i];
    }
    int average = sum/n;
    cout <<"Sum: "<<sum<<endl;
    cout <<"Average: "<<average<<endl;

    return 0;
}
